{
  "manifest_version": 3,
  "name": "LinkedIn Automation",
  "version": "1.0",
  "description": "Auto Like and Comment on Linkedin feed",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://www.linkedin.com/*",
    "http://localhost:3000/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}

