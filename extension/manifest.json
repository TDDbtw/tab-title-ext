{
  "manifest_version": 3,
  "name": "LinkedIn Auto",
  "version": "1.0",
  "description": "Auto Like and Comment Randomly on Linkedin feed",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://www.linkedin.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}

